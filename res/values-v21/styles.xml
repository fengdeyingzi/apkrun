<?xml version="1.0" encoding="utf-8"?>
<resources>

	<style name="fbutton" parent="@android:style/Widget.Button">

		<item name="android:minHeight">50dp</item>		<item name="android:background">@drawable/fbutton</item>
		<item name="android:textColor">#000000</item>
	</style>
	<style name="fdialog" parent="@android:style/Theme.Light">
		<item name="android:backgroundDimAmount">0</item>
        <item name="android:windowBackground">@drawable/button_normal</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
	</style>
	<style name="flist" parent="@android:style/Widget.ListView">
		<item name="android:cacheColorHint">#00000000</item>
		<item name="android:listSelector">@drawable/flist</item>

	</style>




    <style name="f" parent="@android:style/Theme.Material.Light">
		<!--按钮-->

		<item name="android:buttonStyle">@style/fbutton</item>

		<item name="android:listViewStyle">@style/flist</item>
	</style>

	<style name="GrantorNoDisplay" parent="@android:style/Theme.Holo.Light.NoActionBar"> >
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

</resources>
